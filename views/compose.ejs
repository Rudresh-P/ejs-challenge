<%- include('partials/header'); -%>

<h1>Compose</h1>

<form action="/compose" method="post">
    <div class="mb-3">
        <label for="postTitle" class="form-label">Title</label>
        <input name="postTitle" type="text" class="form-control" id="postTitle" autocomplete="off">
      </div>
      <div class="mb-3">
        <label for="postBody" class="form-label">Post</label>
        <textarea name="postBody" class="form-control" id="postBody" rows="3"  autocomplete="off"></textarea>
      </div>
      <button class="btn btn-primary" type="submit">Publish</button>
</form>

<%- include('partials/footer'); -%>